  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "eslint.format.enable": true,
  "files.eol": "\n",
  "cSpell.words": [
    "colour",
    "colours",
    "featuretoggle",
    "financials",
    "generatesession",
    "lcov",
    "Lifespring",
    "Medirecords",
    "mjgaletto",
    "Mrapp",
    "NOSONAR",
    "sonarcloud",
    "svgr",
    "SYNCFUSION",
    "testid",
    "Xmark"
  ]
}
